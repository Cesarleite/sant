#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Meus Sites/santiago-assistant/node_modules/.pnpm/network@0.7.0/node_modules/network/bin/node_modules:/proc/cygdrive/c/Meus Sites/santiago-assistant/node_modules/.pnpm/network@0.7.0/node_modules/network/node_modules:/proc/cygdrive/c/Meus Sites/santiago-assistant/node_modules/.pnpm/network@0.7.0/node_modules:/proc/cygdrive/c/Meus Sites/santiago-assistant/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Meus Sites/santiago-assistant/node_modules/.pnpm/network@0.7.0/node_modules/network/bin/node_modules:/proc/cygdrive/c/Meus Sites/santiago-assistant/node_modules/.pnpm/network@0.7.0/node_modules/network/node_modules:/proc/cygdrive/c/Meus Sites/santiago-assistant/node_modules/.pnpm/network@0.7.0/node_modules:/proc/cygdrive/c/Meus Sites/santiago-assistant/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../network@0.7.0/node_modules/network/bin/network" "$@"
else
  exec node  "$basedir/../../../../../network@0.7.0/node_modules/network/bin/network" "$@"
fi
